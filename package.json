{
  "name": "trivia-api",
  "version": "0.0.0",
  "description": "Trivia API",
  "author": "",
  "license": "MIT",
  "scripts": {
    "all": "source scripts/yarn-all.sh",
    "now-build": "yarn api:build",
    "api:build": "tsc -p projects/trivia-api/tsconfig.json",
    "api:start": "ts-node --project projects/trivia-api/tsconfig.json -r tsconfig-paths/register projects/trivia-api/src/main.ts",
    "api:start:dev": "nodemon --config projects/trivia-api/nodemon.json",
    "api:start:prod": "node dist/api/main.js",
    "api:e2e": "jest --config ./test/jest-e2e.json",
    "api:test": "jest --config ./projects/trivia-api/jest.config.js --watch",
    "api:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "api:test:ci": "jest --config ./projects/trivia-api/jest.config.js --coverage",
    "lint": "prettier --config .prettierrc.json --list-different '{projects,tests}/**/*.{js,json,scss,ts}' && echo 'prettier OK, doing ng lint...' && ng lint",
    "lint-fix": "prettier --config .prettierrc.json --write '{projects,tests}/**/*.{js,json,scss,ts}'; ng lint",
    "start": "ng serve demo-app",
    "build": "ng build -c production demo-app",
    "test": "ng test --watch --project demo-app",
    "test:ci": "ng test --project demo-app",
    "modules:build": "ng build --project ngx-trivia-api",
    "modules:test": "ng test --watch --project ngx-trivia-api",
    "modules:test:ci": "ng test --project ngx-trivia-api"
  },
  "dependencies": {
    "@angular/animations": "7.2.1",
    "@angular/cdk": "7.2.1",
    "@angular/common": "7.2.1",
    "@angular/compiler": "7.2.1",
    "@angular/core": "7.2.1",
    "@angular/forms": "7.2.1",
    "@angular/material": "7.2.1",
    "@angular/platform-browser": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.1",
    "@angular/router": "7.2.1",
    "@nestjs/common": "5.6.2",
    "@nestjs/core": "5.6.2",
    "@ngxs/devtools-plugin": "3.3.4",
    "@ngxs/logger-plugin": "3.3.4",
    "@ngxs/store": "3.3.4",
    "core-js": "2.6.2",
    "ngx-trivia-api": "0.0.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.3.3",
    "zone.js": "0.8.28"
  },
  "devDependencies": {
    "@angular-builders/jest": "7.2.1",
    "@angular-devkit/build-angular": "0.12.2",
    "@angular-devkit/build-ng-packagr": "0.12.2",
    "@angular/cli": "7.2.2",
    "@angular/compiler-cli": "7.2.1",
    "@angular/language-service": "7.2.1",
    "@nestjs/testing": "5.6.2",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "@types/supertest": "2.0.7",
    "codelyzer": "4.5.0",
    "jest": "23.6.0",
    "jest-preset-angular": "6.0.2",
    "ng-packagr": "4.6.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "rxjs-marbles": "5.0.0",
    "supertest": "3.4.1",
    "ts-jest": "23.10.5",
    "ts-loader": "5.3.3",
    "ts-node": "7.0.1",
    "tsconfig-paths": "3.7.0",
    "tsickle": "0.34.0",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "typescript": "3.2.2"
  }
}
